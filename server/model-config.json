{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mysqldb"
  },
  "AccessToken": {
    "dataSource": "mysqldb",
    "public": false
  },
  "ACL": {
    "dataSource": "mysqldb",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mysqldb",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mysqldb",
    "public": false
  },
  "release": {
    "dataSource": "mysqldb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "createRelease": true,
          "getReleases": true,
          "getCollections": true,
          "getTextKeys": true
        }
      }
    }
  },
  "release_collection": {
    "dataSource": "mysqldb",
    "public": true
  },
  "released_text": {
    "dataSource": "mysqldb",
    "public": true
  },
  "branches": {
    "dataSource": "mysqldb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "createBranch": true,
          "listBranches": true,
          "getCollections": true,
          "getTextKeys": true,
          "publish": true,
          "createCollection": true
        }
      }
    }
  },
  "branched_collection": {
    "dataSource": "mysqldb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "publish": true
        }
      }
    }
  },
  "branch_text": {
    "dataSource": "mysqldb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "publish": true,
          "getTexts": true
        }
      }
    }
  },
  "locale": {
    "dataSource": "mysqldb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "create": true,
          "find": true
        }
      }
    }
  },
  "version": {
    "dataSource": "mysqldb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "createVersion": true,
          "getVersion": true,
          "find": true
        }
      }
    }
  }
}
